{
  "Code": {
    "theme": "github",
    "displayLineNumber": false
  },
  "Editor": {
    "theme": "elegant",
    "displayLineNumber": true,
    "vimKeyBinding": false,
    "autoPairCharacters": true,
    "useMarkdownFolding": true,
    "tabSize": 4,
    "indentUnit": 4,
    "indentWithTabs": false,
    "displayActiveLine": true,
    "fontSize": 15,
    "userTheme": "default"
  },
  "General": {
    "enableSyncScroll": true,
    "playKeypressSound": false,
    "enableAutoComplete": true,
    "enableLastFileRestore": false,
    "displayLanguage": "zh-cn",
    "dateFormat": "LLL"
  },
  "Markdown": {
    "tables": true,
    "footnotes": true,
    "breaks": true,
    "sanitize": true,
    "smartLists": true,
    "smartypants": true,
    "mathjax": true,
    "dollarSign": false
  },
  "Viewer": {
    "theme": "github",
    "clickableLink": true,
    "fontSize": 15,
    "userTheme": "default"
  },
  "_version": "0.13.1"
}